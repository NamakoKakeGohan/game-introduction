{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport { ref, computed } from \"vue\";\nimport { useRoute } from \"vue-router\"; // ルート情報を取得\nimport PostItem from \"../components/PostItem.vue\";\nimport postData from \"../postData\";\nimport defaultIcon from \"../assets/userAvatar.png\";\n\n// ルートパラメータを取得\n\nexport default {\n  __name: 'AccountView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const route = useRoute();\n    const userId = route.params.id ? Number(route.params.id) : null; // URLの:idを取得\n\n    // ユーザー情報を取得\n    const user = ref({\n      id: userId || \"仮のID\",\n      name: \"デフォルトユーザー\",\n      icon: defaultIcon\n    });\n\n    // ユーザー情報を`postData`から取得\n    if (userId) {\n      const userData = postData.find(post => post.user.id === userId)?.user;\n      if (userData) {\n        user.value.name = userData.name;\n        user.value.icon = userData.avatar;\n      }\n    }\n\n    // ユーザーの投稿をフィルタリング\n    const userPosts = computed(() => userId ? postData.filter(post => post.user.id === userId) : [] // 仮のIDの場合は空の投稿リスト\n    );\n\n    // ユーザーが「いいね」した投稿（仮のデータ）\n    const likedPosts = ref([{\n      postId: 102,\n      appId: 1234567,\n      appName: \"Liked Game\",\n      appImage: [\"https://via.placeholder.com/150\"],\n      user: {\n        id: userId || \"仮のID\",\n        name: user.value.name,\n        avatar: user.value.icon\n      },\n      tags: [\"タグ1\", \"タグ2\"],\n      oneWard: \"お気に入りのゲーム\",\n      comment: \"このゲームは本当に素晴らしいです！\",\n      steamAppURL: \"https://store.steampowered.com/app/1234567/\",\n      favoriteCount: 100,\n      formattedDate: \"2025年5月15日\"\n    }]);\n\n    // タブ切り替え状態\n    const activeTab = ref(\"accountPosts\");\n    const __returned__ = {\n      route,\n      userId,\n      user,\n      userPosts,\n      likedPosts,\n      activeTab,\n      ref,\n      computed,\n      get useRoute() {\n        return useRoute;\n      },\n      PostItem,\n      get postData() {\n        return postData;\n      },\n      get defaultIcon() {\n        return defaultIcon;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","useRoute","PostItem","postData","defaultIcon","route","userId","params","id","Number","user","name","icon","userData","find","post","value","avatar","userPosts","filter","likedPosts","postId","appId","appName","appImage","tags","oneWard","comment","steamAppURL","favoriteCount","formattedDate","activeTab"],"sources":["/Users/namako-mac/mysite/game-introduction/src/views/AccountView.vue"],"sourcesContent":["<template>\n  <div class=\"account\">\n    <!-- ユーザー情報 -->\n    <div class=\"user-info\">\n      <img :src=\"user.icon\" alt=\"User Icon\" class=\"user-icon\" />\n      <div>\n        <h1 class=\"user-name\">{{ user.name }}</h1>\n        <p class=\"user-id\">@{{ user.id }}</p>\n      </div>\n    </div>\n\n    <!-- タブ切り替え -->\n    <div class=\"tab-toggle\">\n      <button :class=\"{ active: activeTab === 'accountPosts' }\" @click=\"activeTab = 'accountPosts'\">\n        投稿\n      </button>\n      <button :class=\"{ active: activeTab === 'likes' }\" @click=\"activeTab = 'likes'\">\n        いいね\n      </button>\n    </div>\n\n    <!-- 投稿表示 -->\n    <div v-if=\"activeTab === 'accountPosts'\" class=\"post-list\">\n      <PostItem :posts=\"userPosts\" />\n    </div>\n    <div v-else class=\"post-list\">\n      <PostItem :posts=\"likedPosts\" />\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, computed } from \"vue\";\nimport { useRoute }      from \"vue-router\"; // ルート情報を取得\nimport PostItem          from \"../components/PostItem.vue\";\nimport postData          from \"../postData\";\nimport defaultIcon       from \"../assets/userAvatar.png\";\n\n// ルートパラメータを取得\nconst route = useRoute();\nconst userId = route.params.id ? Number(route.params.id) : null; // URLの:idを取得\n\n// ユーザー情報を取得\nconst user = ref({\n  id: userId || \"仮のID\",\n  name: \"デフォルトユーザー\",\n  icon: defaultIcon,\n});\n\n// ユーザー情報を`postData`から取得\nif (userId) {\n  const userData = postData.find((post) => post.user.id === userId)?.user;\n  if (userData) {\n    user.value.name = userData.name;\n    user.value.icon = userData.avatar;\n  }\n}\n\n// ユーザーの投稿をフィルタリング\nconst userPosts = computed(() =>\n  userId\n    ? postData.filter((post) => post.user.id === userId)\n    : [] // 仮のIDの場合は空の投稿リスト\n);\n\n// ユーザーが「いいね」した投稿（仮のデータ）\nconst likedPosts = ref([\n  {\n    postId: 102,\n    appId: 1234567,\n    appName: \"Liked Game\",\n    appImage: [\"https://via.placeholder.com/150\"],\n    user: {\n      id: userId || \"仮のID\",\n      name: user.value.name,\n      avatar: user.value.icon,\n    },\n    tags: [\"タグ1\", \"タグ2\"],\n    oneWard: \"お気に入りのゲーム\",\n    comment: \"このゲームは本当に素晴らしいです！\",\n    steamAppURL: \"https://store.steampowered.com/app/1234567/\",\n    favoriteCount: 100,\n    formattedDate: \"2025年5月15日\",\n  },\n]);\n\n// タブ切り替え状態\nconst activeTab = ref(\"accountPosts\");\n</script>\n\n<style scoped>\n.account {\n  display: flex;\n  flex-direction: column;\n  margin-right: 228px;\n  padding: 20px;\n}\n\n.user-info {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  gap: 15px;\n  margin-bottom: 15px;\n}\n\n.user-icon {\n  width: 70px;\n  height: 70px;\n  border-radius: 50%;\n}\n\n.user-name {\n  font-size: 2rem;\n  font-weight: bold;\n  margin: 0;\n}\n\n.user-id {\n  color: #657786;\n  font-size: 0.9rem;\n  margin: 2px 0 0;\n}\n\n.tab-toggle {\n  display: flex;\n  justify-content: center;\n  gap: 20px;\n  margin-bottom: 15px;\n  border-bottom: 1px solid #ccc;\n}\n\n.tab-toggle button {\n  background: none;\n  border: none;\n  padding: 10px 20px;\n  font-size: 1.5rem;\n  cursor: pointer;\n  color: #657786;\n  border-bottom: 2px solid transparent;\n  transition: all 0.3s ease;\n}\n\n.tab-toggle button.active {\n  color: #1da1f2;\n  border-color: #1da1f2;\n  font-weight: bold;\n}\n\n.post-list {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  margin-top: 30px;\n  gap: 15px;\n}\n</style>\n"],"mappings":";;;AAgCA,SAASA,GAAG,EAAEC,QAAQ,QAAQ,KAAK;AACnC,SAASC,QAAQ,QAAa,YAAY,CAAC,CAAC;AAC5C,OAAOC,QAAQ,MAAe,4BAA4B;AAC1D,OAAOC,QAAQ,MAAe,aAAa;AAC3C,OAAOC,WAAW,MAAY,0BAA0B;;AAExD;;;;;;;;IACA,MAAMC,KAAK,GAAGJ,QAAQ,CAAC,CAAC;IACxB,MAAMK,MAAM,GAAGD,KAAK,CAACE,MAAM,CAACC,EAAE,GAAGC,MAAM,CAACJ,KAAK,CAACE,MAAM,CAACC,EAAE,CAAC,GAAG,IAAI,CAAC,CAAC;;IAEjE;IACA,MAAME,IAAI,GAAGX,GAAG,CAAC;MACfS,EAAE,EAAEF,MAAM,IAAI,MAAM;MACpBK,IAAI,EAAE,WAAW;MACjBC,IAAI,EAAER;IACR,CAAC,CAAC;;IAEF;IACA,IAAIE,MAAM,EAAE;MACV,MAAMO,QAAQ,GAAGV,QAAQ,CAACW,IAAI,CAAEC,IAAI,IAAKA,IAAI,CAACL,IAAI,CAACF,EAAE,KAAKF,MAAM,CAAC,EAAEI,IAAI;MACvE,IAAIG,QAAQ,EAAE;QACZH,IAAI,CAACM,KAAK,CAACL,IAAI,GAAGE,QAAQ,CAACF,IAAI;QAC/BD,IAAI,CAACM,KAAK,CAACJ,IAAI,GAAGC,QAAQ,CAACI,MAAM;MACnC;IACF;;IAEA;IACA,MAAMC,SAAS,GAAGlB,QAAQ,CAAC,MACzBM,MAAK,GACDH,QAAQ,CAACgB,MAAM,CAAEJ,IAAI,IAAKA,IAAI,CAACL,IAAI,CAACF,EAAE,KAAKF,MAAM,IACjD,EAAE,CAAC;IACT,CAAC;;IAED;IACA,MAAMc,UAAU,GAAGrB,GAAG,CAAC,CACrB;MACEsB,MAAM,EAAE,GAAG;MACXC,KAAK,EAAE,OAAO;MACdC,OAAO,EAAE,YAAY;MACrBC,QAAQ,EAAE,CAAC,iCAAiC,CAAC;MAC7Cd,IAAI,EAAE;QACJF,EAAE,EAAEF,MAAM,IAAI,MAAM;QACpBK,IAAI,EAAED,IAAI,CAACM,KAAK,CAACL,IAAI;QACrBM,MAAM,EAAEP,IAAI,CAACM,KAAK,CAACJ;MACrB,CAAC;MACDa,IAAI,EAAE,CAAC,KAAK,EAAE,KAAK,CAAC;MACpBC,OAAO,EAAE,WAAW;MACpBC,OAAO,EAAE,mBAAmB;MAC5BC,WAAW,EAAE,6CAA6C;MAC1DC,aAAa,EAAE,GAAG;MAClBC,aAAa,EAAE;IACjB,CAAC,CACF,CAAC;;IAEF;IACA,MAAMC,SAAS,GAAGhC,GAAG,CAAC,cAAc,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}