{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport { ref, computed } from \"vue\";\nimport { useRoute } from \"vue-router\";\nimport PostItem from \"../components/PostItem.vue\";\nimport postData from \"../postData\";\nimport defaultIcon from \"../assets/userAvatar.png\";\n\n// ルートパラメータから id を取得\n\nexport default {\n  __name: 'AccountView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const route = useRoute();\n    const userId = route.params.id ? Number(route.params.id) : null;\n\n    // 該当ユーザーの投稿を抽出\n    const userPosts = computed(() => postData.filter(post => post.user.id === userId));\n\n    // 最初の投稿からユーザー情報を取得（なければデフォルト）\n    const user = computed(() => {\n      const firstPost = userPosts.value[0];\n      if (firstPost) {\n        return {\n          id: firstPost.user.id,\n          name: firstPost.user.name,\n          icon: firstPost.user.avatar\n        };\n      } else {\n        return {\n          id: userId,\n          name: \"不明なユーザー\",\n          icon: defaultIcon\n        };\n      }\n    });\n\n    // タブ切り替え状態\n    const activeTab = ref(\"accountPosts\");\n\n    // 仮のいいねデータ（将来的にIDと紐づけたい場合はフィルターで処理）\n    const likedPosts = ref([{\n      postId: 102,\n      appId: 1234567,\n      appName: \"Liked Game\",\n      appImage: [\"https://via.placeholder.com/150\"],\n      user: {\n        id: 999999,\n        name: \"他のユーザー\",\n        avatar: require(\"../assets/userAvatar.png\")\n      },\n      tags: [\"タグ1\", \"タグ2\"],\n      oneWard: \"お気に入りのゲーム\",\n      comment: \"このゲームは本当に素晴らしいです！\",\n      steamAppURL: \"https://store.steampowered.com/app/1234567/\",\n      favoriteCount: 100,\n      formattedDate: \"2025年5月15日\"\n    }]);\n    const __returned__ = {\n      route,\n      userId,\n      userPosts,\n      user,\n      activeTab,\n      likedPosts,\n      ref,\n      computed,\n      get useRoute() {\n        return useRoute;\n      },\n      PostItem,\n      get postData() {\n        return postData;\n      },\n      get defaultIcon() {\n        return defaultIcon;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","useRoute","PostItem","postData","defaultIcon","route","userId","params","id","Number","userPosts","filter","post","user","firstPost","value","name","icon","avatar","activeTab","likedPosts","postId","appId","appName","appImage","require","tags","oneWard","comment","steamAppURL","favoriteCount","formattedDate"],"sources":["/Users/namako-mac/mysite/game-introduction/src/views/AccountView.vue"],"sourcesContent":["<template>\n  <div class=\"account\">\n    <!-- ユーザー情報 -->\n    <div class=\"user-info\">\n      <img :src=\"user.icon\" alt=\"User Icon\" class=\"user-icon\" />\n      <div>\n        <h1 class=\"user-name\">{{ user.name }}</h1>\n        <p class=\"user-id\">@{{ user.id }}</p>\n      </div>\n    </div>\n    <!-- タブ切り替え -->\n    <div class=\"tab-toggle\">\n      <button :class=\"{ active: activeTab === 'accountPosts' }\" @click=\"activeTab = 'accountPosts'\">\n        投稿\n      </button>\n      <button :class=\"{ active: activeTab === 'likes' }\" @click=\"activeTab = 'likes'\">\n        いいね\n      </button>\n    </div>\n    <!-- 投稿表示 -->\n    <div v-if=\"activeTab === 'accountPosts'\" class=\"post-list\">\n      <PostItem :posts=\"userPosts\" />\n    </div>\n    <div v-else class=\"post-list\">\n      <PostItem :posts=\"likedPosts\" />\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, computed } from \"vue\";\nimport { useRoute }      from \"vue-router\";\nimport PostItem          from \"../components/PostItem.vue\";\nimport postData          from \"../postData\";\nimport defaultIcon       from \"../assets/userAvatar.png\";\n\n// ルートパラメータから id を取得\nconst route = useRoute();\nconst userId = route.params.id ? Number(route.params.id) : null;\n\n// 該当ユーザーの投稿を抽出\nconst userPosts = computed(() =>\n  postData.filter((post) => post.user.id === userId)\n);\n\n// 最初の投稿からユーザー情報を取得（なければデフォルト）\nconst user = computed(() => {\n  const firstPost = userPosts.value[0];\n  if (firstPost) {\n    return {\n      id: firstPost.user.id,\n      name: firstPost.user.name,\n      icon: firstPost.user.avatar\n    };\n  } else {\n    return {\n      id: userId,\n      name: \"不明なユーザー\",\n      icon: defaultIcon,\n    };\n  }\n});\n\n// タブ切り替え状態\nconst activeTab = ref(\"accountPosts\");\n\n// 仮のいいねデータ（将来的にIDと紐づけたい場合はフィルターで処理）\nconst likedPosts = ref([\n  {\n    postId: 102,\n    appId: 1234567,\n    appName: \"Liked Game\",\n    appImage: [\"https://via.placeholder.com/150\"],\n    user: {\n      id: 999999,\n      name: \"他のユーザー\",\n      avatar: require(\"../assets/userAvatar.png\"),\n    },\n    tags: [\"タグ1\", \"タグ2\"],\n    oneWard: \"お気に入りのゲーム\",\n    comment: \"このゲームは本当に素晴らしいです！\",\n    steamAppURL: \"https://store.steampowered.com/app/1234567/\",\n    favoriteCount: 100,\n    formattedDate: \"2025年5月15日\",\n  },\n]);\n</script>\n\n\n<style scoped>\n\n.account {\n  display: flex;\n  flex-direction: column;\n  margin-right: 228px;\n  padding: 20px;\n}\n\n.user-info {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  gap: 15px;\n  margin-bottom: 15px;\n}\n\n.user-icon {\n  width: 70px;\n  height: 70px;\n  border-radius: 50%;\n}\n\n.user-name {\n  font-size: 2rem;\n  font-weight: bold;\n  margin: 0;\n}\n\n.user-id {\n  color: #657786;\n  font-size: 0.9rem;\n  margin: 2px 0 0;\n}\n\n.tab-toggle {\n  display: flex;\n  justify-content: center;\n  gap: 20px;\n  margin-bottom: 15px;\n  border-bottom: 1px solid #ccc;\n}\n\n.tab-toggle button {\n  background: none;\n  border: none;\n  padding: 10px 20px;\n  font-size: 1.5rem;\n  cursor: pointer;\n  color: #657786;\n  border-bottom: 2px solid transparent;\n  transition: all 0.3s ease;\n}\n\n.tab-toggle button.active {\n  color: #1da1f2;\n  border-color: #1da1f2;\n  font-weight: bold;\n}\n\n.post-list {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  margin-top: 30px;\n  gap: 15px;\n}\n</style>\n"],"mappings":";;AA8BA,SAASA,GAAG,EAAEC,QAAQ,QAAQ,KAAK;AACnC,SAASC,QAAQ,QAAa,YAAY;AAC1C,OAAOC,QAAQ,MAAe,4BAA4B;AAC1D,OAAOC,QAAQ,MAAe,aAAa;AAC3C,OAAOC,WAAW,MAAY,0BAA0B;;AAExD;;;;;;;;IACA,MAAMC,KAAK,GAAGJ,QAAQ,CAAC,CAAC;IACxB,MAAMK,MAAM,GAAGD,KAAK,CAACE,MAAM,CAACC,EAAE,GAAGC,MAAM,CAACJ,KAAK,CAACE,MAAM,CAACC,EAAE,CAAC,GAAG,IAAI;;IAE/D;IACA,MAAME,SAAS,GAAGV,QAAQ,CAAC,MACzBG,QAAQ,CAACQ,MAAM,CAAEC,IAAI,IAAKA,IAAI,CAACC,IAAI,CAACL,EAAE,KAAKF,MAAM,CACnD,CAAC;;IAED;IACA,MAAMO,IAAI,GAAGb,QAAQ,CAAC,MAAM;MAC1B,MAAMc,SAAS,GAAGJ,SAAS,CAACK,KAAK,CAAC,CAAC,CAAC;MACpC,IAAID,SAAS,EAAE;QACb,OAAO;UACLN,EAAE,EAAEM,SAAS,CAACD,IAAI,CAACL,EAAE;UACrBQ,IAAI,EAAEF,SAAS,CAACD,IAAI,CAACG,IAAI;UACzBC,IAAI,EAAEH,SAAS,CAACD,IAAI,CAACK;QACvB,CAAC;MACH,CAAC,MAAM;QACL,OAAO;UACLV,EAAE,EAAEF,MAAM;UACVU,IAAI,EAAE,SAAS;UACfC,IAAI,EAAEb;QACR,CAAC;MACH;IACF,CAAC,CAAC;;IAEF;IACA,MAAMe,SAAS,GAAGpB,GAAG,CAAC,cAAc,CAAC;;IAErC;IACA,MAAMqB,UAAU,GAAGrB,GAAG,CAAC,CACrB;MACEsB,MAAM,EAAE,GAAG;MACXC,KAAK,EAAE,OAAO;MACdC,OAAO,EAAE,YAAY;MACrBC,QAAQ,EAAE,CAAC,iCAAiC,CAAC;MAC7CX,IAAI,EAAE;QACJL,EAAE,EAAE,MAAM;QACVQ,IAAI,EAAE,QAAQ;QACdE,MAAM,EAAEO,OAAO,CAAC,0BAA0B;MAC5C,CAAC;MACDC,IAAI,EAAE,CAAC,KAAK,EAAE,KAAK,CAAC;MACpBC,OAAO,EAAE,WAAW;MACpBC,OAAO,EAAE,mBAAmB;MAC5BC,WAAW,EAAE,6CAA6C;MAC1DC,aAAa,EAAE,GAAG;MAClBC,aAAa,EAAE;IACjB,CAAC,CACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}