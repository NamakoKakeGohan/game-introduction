/components/PostFunctionModalDialog.vue"],"sourcesContent":["<template>\n  <teleport to=\"body\">\n    <div v-if=\"openModal\" class=\"modal-overlay\">\n      <article class=\"modal\">\n        <button class=\"modal__close\" @click=\"$emit('close')\">×</button>\n\n        <main class=\"modal__body\">\n          <section class=\"modal__content\">\n            <!-- 左側: サムネイル -->\n            <aside class=\"modal__thumbnail\">\n              <img\n                :src=\"Steam_top\"\n                alt=\"App thumbnail\"\n                class=\"thumbnail-image\"\n              />\n            </aside>\n\n            <!-- 右側: 詳細 -->\n            <section class=\"modal__details\">\n              <header class=\"details__header\">\n                <input\n                  v-model=\"appName\"\n                  placeholder=\"ゲームタイトルを入力\"\n                  class=\"details__app-name\"\n                />\n              </header>\n\n              <section class=\"details__user\">\n                <img :src=\"userAvatar\" alt=\"User Avatar\" class=\"user-avatar\" />\n                <h4 class=\"user-name\">{{ userName }}</h4>\n                <span class=\"user-id\">{{ userId }}</span>\n              </section>\n\n              <section class=\"details__summary\">\n                <textarea\n                  v-model=\"oneWord\"\n                  placeholder=\"このゲームを一言で表すと？\"\n                  class=\"summary-text\"\n                ></textarea>\n              </section>\n\n              <section class=\"details__comment\">\n                <textarea\n                  v-model=\"comment\"\n                  placeholder=\"どんな人に勧めたいゲーム . . . ？ \"\n                  class=\"comment-input\"\n                ></textarea>\n              </section>\n\n              <footer class=\"details__actions\">\n                <button @click=\"submitPost\" class=\"submit-post-btn\">投稿する</button>\n              </footer>\n            </section>\n          </section>\n        </main>\n      </article>\n    </div>\n  </teleport>\n</template>\n\n<script setup>\nimport { ref } from \"vue\";\nimport userAvatar from \"../assets/userAvatar.png\";\nimport Steam_ from \"../assets/thumbnail/Steam_top.png\";\nimport Steam_top from \"../assets/thumbnail/Steam_top.png\";\n\nconst props = defineProps({\n  openModal: Boolean,\n});\n\nconst emit = defineEmits([\"close\", \"submitPost\"]);\n\nconst userName = \"ゲスト\";\nconst userId = 255;\n\nconst appName = ref(\"\");\nconst tag1 = ref(\"\");\nconst tag2 = ref(\"\");\nconst oneWord = ref(\"\");\nconst comment = ref(\"\");\n\nfunction submitPost() {\n  if (!appName.value || !oneWord.value || !comment.value) {\n    alert(\"すべてのフィールドを入力してください！\");\n    return;\n  }\n\n  emit(\"submitPost\", {\n    appName: appName.value,\n    oneWord: oneWord.value,\n    comment: comment.value,\n    thumbnail: Steam_top,\n    user: {\n      id: userId,\n      name: userName,\n      avatar: userAvatar,\n    },\n  });\n\n  emit(\"close\");\n}\n</script>\n\n<style scoped>\n.modal-overlay {\n  position: fixed;\n  inset: 0;\n  display: grid;\n  place-items: center;\n  background: rgba(0 0 0 / 0.5);\n  z-index: 999;\n}\n.modal {\n  position: relative;\n  width: 90vw;\n  max-width: 900px;\n  background: #fff;\n  border-radius: 12px;\n  box-shadow: 0 4px 24px rgba(0 0 0 / 0.18);\n}\n.modal__close {\n  position: absolute;\n  top: 18px;\n  right: 18px;\n  width: 36px;\n  aspect-ratio: 1;\n  display: grid;\n  place-items: center;\n  border: none;\n  border-radius: 50%;\n  background: #ff6b6b;\n  color: #fff;\n  font-size: 22px;\n  font-weight: bold;\n  cursor: pointer;\n  transition: background 0.2s, transform 0.2s;\n}\n.modal__close:hover {\n  background: #ff4c4c;\n  transform: scale(1.1);\n}\n.modal__body {\n  display: flex;\n  flex-direction: column;\n  padding: 40px 32px 32px;\n  gap: 24px;\n}\n.modal__content {\n  display: flex;\n  gap: 32px;\n}\n.modal__thumbnail {\n  flex: 1 1 40%;\n  display: flex;\n  justify-content: center;\n}\n.thumbnail-image {\n  width: 100%;\n  max-width: 320px;\n  border-radius: 10px;\n  object-fit: cover;\n  box-shadow: 0 2px 8px rgba(0 0 0 / 0.08);\n}\n.modal__details {\n  flex: 1 1 60%;\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n}\n.details__app-name {\n  width: 100%;\n  padding: 8px 12px;\n  font-size: 1.6rem;\n  font-weight: bold;\n  border: 1px solid #ccc;\n  border-radius: 6px;\n  color: #333;\n}\n.details__user {\n  display: flex;\n  align-items: center;\n  gap: 10px;\n}\n.user-avatar {\n  width: 32px;\n  aspect-ratio: 1;\n  border-radius: 50%;\n}\n.user-name {\n  flex: 1;\n  padding: 6px 10px;\n  font-size: 1rem;\n}\n.user-id {\n  margin-left: 12px;\n  color: #888;\n  font-size: 0.95rem;\n}\n.details__summary .summary-text {\n  width: 100%;\n  padding: 10px 16px;\n  font-size: 1.1rem;\n  border: 2px solid #265e9e;\n  border-radius: 5px;\n  background: #f5fbff;\n  resize: none;\n}\n.details__comment .comment-input {\n  width: 100%;\n  min-height: 80px;\n  padding: 8px;\n  font-size: 1rem;\n  border: 1px solid #ccc;\n  border-radius: 6px;\n  resize: none;\n}\n.details__actions {\n  display: flex;\n  justify-content: flex-end;\n}\n.submit-post-btn {\n  margin-top: 12px;\n  padding: 10px 24px;\n  font-size: 1.1rem;\n  border: none;\n  border-radius: 6px;\n  background: #265e9e;\n  color: #fff;\n  cursor: pointer;\n  transition: background 0.2s;\n}\n.submit-post-btn:hover {\n  background: #183e6e;\n}\n@media (max-width: 800px) {\n  .modal__body {\n    padding: 24px 8px 16px;\n    gap: 16px;\n  }\n  .modal__content {\n    flex-direction: column;\n    gap: 16px;\n  }\n  .thumbnail-image {\n    max-width: 100%;\n  }\n}\n</style>\n"],"mappings":";;EAAAA,GAAA;EAE0BC,KAAK,EAAC;;;EACjBA,KAAK,EAAC;AAAO;;EAGdA,KAAK,EAAC;AAAa;;EACdA,KAAK,EAAC;AAAgB;;EAEtBA,KAAK,EAAC;AAAkB;mBAT3C;;EAkBqBA,KAAK,EAAC;AAAgB;;EACrBA,KAAK,EAAC;AAAiB;;EAQtBA,KAAK,EAAC;AAAe;oBA3B5C;;EAiCuBA,KAAK,EAAC;AAAkB;;EAQxBA,KAAK,EAAC;AAAkB;;uBAxC7CC,YAAA,CAwDWC,SAAA;IAxDDC,EAAE,EAAC;EAAM,IACNC,MAAA,CA