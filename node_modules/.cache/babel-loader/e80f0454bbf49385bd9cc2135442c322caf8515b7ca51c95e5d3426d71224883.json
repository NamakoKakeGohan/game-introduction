{"ast":null,"code":"import { computed } from \"vue\";\nimport postData from \"../postData\";\n\n// ランキング上位10件を計算\n\nexport default {\n  __name: 'RankingView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const top10Posts = computed(() => {\n      return [...postData].sort((a, b) => b.likeCount - a.likeCount) // likeCountで降順ソート\n      .slice(0, 10); // 上位10件を取得\n    });\n    const __returned__ = {\n      top10Posts,\n      computed,\n      get postData() {\n        return postData;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["computed","postData","top10Posts","sort","a","b","likeCount","slice"],"sources":["/Users/namako-mac/mysite/game-introduction/src/views/RankingView.vue"],"sourcesContent":["<template>\n  <div class=\"ranking\">\n    <h1>ランキング</h1>\n    <ul class=\"ranking-list\">\n      <li v-for=\"(post, index) in top10Posts\" :key=\"post.postId\" class=\"ranking-item\">\n        <div class=\"ranking-thumbnail\">\n          <img :src=\"post.appImage[0]\" alt=\"Thumbnail\" />\n        </div>\n        <div class=\"ranking-info\">\n          <h2 class=\"app-name\">{{ index + 1 }}位: {{ post.appName }}</h2>\n          <p class=\"like-count\">いいね数: {{ post.likeCount }}</p>\n        </div>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script setup>\nimport { computed } from \"vue\";\nimport postData from \"../postData\";\n\n// ランキング上位10件を計算\nconst top10Posts = computed(() => {\n  return [...postData]\n    .sort((a, b) => b.likeCount - a.likeCount) // likeCountで降順ソート\n    .slice(0, 10); // 上位10件を取得\n});\n</script>\n\n<style scoped>\n.ranking {\n  padding: 20px;\n  max-width: 800px;\n  margin: 0 auto;\n}\n\n.ranking-list {\n  list-style: none;\n  padding: 0;\n}\n\n.ranking-item {\n  display: flex;\n  align-items: center;\n  margin-bottom: 20px;\n  border-bottom: 1px solid #ddd;\n  padding-bottom: 10px;\n}\n\n.ranking-thumbnail img {\n  width: 100px;\n  height: 100px;\n  object-fit: cover;\n  border-radius: 8px;\n  margin-right: 20px;\n}\n\n.ranking-info {\n  flex: 1;\n}\n\n.app-name {\n  font-size: 1.2rem;\n  font-weight: bold;\n  margin: 0;\n}\n\n.like-count {\n  font-size: 1rem;\n  color: #555;\n}\n</style>"],"mappings":"AAkBA,SAASA,QAAQ,QAAQ,KAAK;AAC9B,OAAOC,QAAQ,MAAM,aAAa;;AAElC;;;;;;;;IACA,MAAMC,UAAU,GAAGF,QAAQ,CAAC,MAAM;MAChC,OAAO,CAAC,GAAGC,QAAQ,EAChBE,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKA,CAAC,CAACC,SAAS,GAAGF,CAAC,CAACE,SAAS,CAAC,CAAC;MAAA,CAC1CC,KAAK,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC;IACnB,CAAC,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}