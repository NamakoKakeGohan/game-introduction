{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport { ref, computed } from \"vue\";\nimport PostItem from \"../components/PostItem.vue\";\nimport postData from \"../postData\";\n\n// タブ切り替え状態\n\nexport default {\n  __name: 'AccountView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const activeTab = ref(\"posts\");\n    // ユーザーの投稿をフィルタリング\n    const userPosts = computed(() => postData.filter(post => post.user.id === user.value.id));\n    // ユーザー情報を指定\n    const user = ref({\n      id: 123456,\n      name: \"じょん・すみす\",\n      icon: require(\"../assets/userAvatar.png\") // ユーザーアイコン\n    });\n\n    // ユーザーが「いいね」した投稿（仮のデータ）\n    const likedPosts = ref([{\n      postId: 102,\n      appId: 1234567,\n      appName: \"Liked Game\",\n      appImage: [\"https://via.placeholder.com/150\"],\n      user: {\n        id: 123456,\n        name: \"じょん・すみす\",\n        avatar: require(\"../assets/userAvatar.png\")\n      },\n      tags: [\"タグ1\", \"タグ2\"],\n      oneWard: \"お気に入りのゲーム\",\n      comment: \"このゲームは本当に素晴らしいです！\",\n      steamAppURL: \"https://store.steampowered.com/app/1234567/\",\n      favoriteCount: 100,\n      formattedDate: \"2025年5月15日\"\n    }]);\n    const __returned__ = {\n      activeTab,\n      userPosts,\n      user,\n      likedPosts,\n      ref,\n      computed,\n      PostItem,\n      get postData() {\n        return postData;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","PostItem","postData","activeTab","userPosts","filter","post","user","id","value","name","icon","require","likedPosts","postId","appId","appName","appImage","avatar","tags","oneWard","comment","steamAppURL","favoriteCount","formattedDate"],"sources":["/Users/namako-mac/mysite/game-introduction/src/views/AccountView.vue"],"sourcesContent":["<template>\n  <div class=\"account\">\n    <!-- ユーザー情報 -->\n    <div class=\"user-info\">\n      <img :src=\"user.icon\" alt=\"User Icon\" class=\"user-icon\" />\n      <div>\n        <h1 class=\"user-name\">{{ user.name }}</h1>\n        <p class=\"user-id\">@{{ user.id }}</p>\n      </div>\n    </div>\n\n    <!-- タブ切り替え -->\n    <div class=\"tab-toggle\">\n      <button :class=\"{ active: activeTab === 'posts' }\" @click=\"activeTab = 'posts'\" >\n        投稿\n      </button>\n      <button :class=\"{ active: activeTab === 'likes' }\" @click=\"activeTab = 'likes'\" >\n        いいね\n      </button>\n    </div>\n\n    <!-- 投稿表示 -->\n    <div v-if=\"activeTab === 'posts'\" class=\"post-list\">\n      <PostItem :posts=\"userPosts\" class=\"post\"/>\n    </div>\n    <div v-else class=\"post-list\">\n      <PostItem :posts=\"likedPosts\" class=\"post\"/>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, computed } from \"vue\";\nimport PostItem from \"../components/PostItem.vue\";\nimport postData from \"../postData\";\n\n// タブ切り替え状態\nconst activeTab = ref(\"posts\");\n// ユーザーの投稿をフィルタリング\nconst userPosts = computed(() =>\n  postData.filter((post) => post.user.id === user.value.id)\n);\n// ユーザー情報を指定\nconst user = ref({\n  id: 123456,\n  name: \"じょん・すみす\",\n  icon: require(\"../assets/userAvatar.png\"), // ユーザーアイコン\n});\n\n// ユーザーが「いいね」した投稿（仮のデータ）\nconst likedPosts = ref([\n  {\n    postId: 102,\n    appId: 1234567,\n    appName: \"Liked Game\",\n    appImage: [\"https://via.placeholder.com/150\"],\n    user: {\n      id: 123456,\n      name: \"じょん・すみす\",\n      avatar: require(\"../assets/userAvatar.png\"),\n    },\n    tags: [\"タグ1\", \"タグ2\"],\n    oneWard: \"お気に入りのゲーム\",\n    comment: \"このゲームは本当に素晴らしいです！\",\n    steamAppURL: \"https://store.steampowered.com/app/1234567/\",\n    favoriteCount: 100,\n    formattedDate: \"2025年5月15日\",\n  },\n]);\n\n</script>\n\n<style scoped>\n.account {\n  \n  padding: 20px;\n  max-width: 80%;\n  margin-left: 90px;\n}\n\n.user-info {\n  display: flex;\n  align-items: center;\n  gap: 15px;\n  margin-bottom: 15px;\n}\n\n.user-icon {\n  width: 60px;\n  height: 60px;\n  border-radius: 50%;\n}\n\n.user-name {\n  font-size: 1.2rem;\n  font-weight: bold;\n  margin: 0;\n}\n\n.user-id {\n  color: #657786;\n  font-size: 0.9rem;\n  margin: 2px 0 0;\n}\n\n.tab-toggle {\n  display: flex;\n  gap: 20px;\n  margin-bottom: 15px;\n  border-bottom: 1px solid #ccc;\n}\n\n.tab-toggle button {\n  background: none;\n  border: none;\n  padding: 10px 0;\n  font-size: 1.35rem;\n  cursor: pointer;\n  color: #657786;\n  border-bottom: 2px solid transparent;\n  transition: all 0.3s ease;\n}\n\n.tab-toggle button.active {\n  color: #1da1f2;\n  border-color: #1da1f2;\n  font-weight: bold;\n}\n\n.post-list {\n  display: flex;\n  flex-direction: column;\n\n  gap: 15px;\n}\n.post {\n  margin-top: 50px;\n}\n</style>\n"],"mappings":";;AAgCA,SAASA,GAAG,EAAEC,QAAQ,QAAQ,KAAK;AACnC,OAAOC,QAAQ,MAAM,4BAA4B;AACjD,OAAOC,QAAQ,MAAM,aAAa;;AAElC;;;;;;;;IACA,MAAMC,SAAS,GAAGJ,GAAG,CAAC,OAAO,CAAC;IAC9B;IACA,MAAMK,SAAS,GAAGJ,QAAQ,CAAC,MACzBE,QAAQ,CAACG,MAAM,CAAEC,IAAI,IAAKA,IAAI,CAACC,IAAI,CAACC,EAAE,KAAKD,IAAI,CAACE,KAAK,CAACD,EAAE,CAC1D,CAAC;IACD;IACA,MAAMD,IAAI,GAAGR,GAAG,CAAC;MACfS,EAAE,EAAE,MAAM;MACVE,IAAI,EAAE,SAAS;MACfC,IAAI,EAAEC,OAAO,CAAC,0BAA0B,CAAC,CAAE;IAC7C,CAAC,CAAC;;IAEF;IACA,MAAMC,UAAU,GAAGd,GAAG,CAAC,CACrB;MACEe,MAAM,EAAE,GAAG;MACXC,KAAK,EAAE,OAAO;MACdC,OAAO,EAAE,YAAY;MACrBC,QAAQ,EAAE,CAAC,iCAAiC,CAAC;MAC7CV,IAAI,EAAE;QACJC,EAAE,EAAE,MAAM;QACVE,IAAI,EAAE,SAAS;QACfQ,MAAM,EAAEN,OAAO,CAAC,0BAA0B;MAC5C,CAAC;MACDO,IAAI,EAAE,CAAC,KAAK,EAAE,KAAK,CAAC;MACpBC,OAAO,EAAE,WAAW;MACpBC,OAAO,EAAE,mBAAmB;MAC5BC,WAAW,EAAE,6CAA6C;MAC1DC,aAAa,EAAE,GAAG;MAClBC,aAAa,EAAE;IACjB,CAAC,CACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}