{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport { ref, computed } from \"vue\";\nimport { useRoute } from \"vue-router\"; // ルート情報を取得\nimport PostItem from \"../components/PostItem.vue\";\nimport postData from \"../postData\";\nimport defaultI from \"../assets/userAvatar.png\";\n\n// ルートパラメータを取得\n\nexport default {\n  __name: 'AccountView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const route = useRoute();\n    const userId = route.params.id ? Number(route.params.id) : \"仮のID\";\n    const userName = route.params.name || \"デフォルトユーザー\";\n    const userIcon = route.params.icon || require();\n\n    // ユーザー情報を指定\n    const user = ref({\n      id: userId,\n      name: userName,\n      icon: userIcon\n    });\n\n    // ユーザーの投稿をフィルタリング\n    const userPosts = computed(() => typeof userId === \"number\" ? postData.filter(post => post.user.id === user.value.id) : [] // 仮のIDの場合は空の投稿リスト\n    );\n\n    // ユーザーが「いいね」した投稿（仮のデータ）\n    const likedPosts = ref([{\n      postId: 102,\n      appId: 1234567,\n      appName: \"Liked Game\",\n      appImage: [\"https://via.placeholder.com/150\"],\n      user: {\n        id: userId,\n        name: userName,\n        avatar: userIcon\n      },\n      tags: [\"タグ1\", \"タグ2\"],\n      oneWard: \"お気に入りのゲーム\",\n      comment: \"このゲームは本当に素晴らしいです！\",\n      steamAppURL: \"https://store.steampowered.com/app/1234567/\",\n      favoriteCount: 100,\n      formattedDate: \"2025年5月15日\"\n    }]);\n\n    // タブ切り替え状態\n    const activeTab = ref(\"accountPosts\");\n    const __returned__ = {\n      route,\n      userId,\n      userName,\n      userIcon,\n      user,\n      userPosts,\n      likedPosts,\n      activeTab,\n      ref,\n      computed,\n      get useRoute() {\n        return useRoute;\n      },\n      PostItem,\n      get postData() {\n        return postData;\n      },\n      get defaultI() {\n        return defaultI;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","useRoute","PostItem","postData","defaultI","route","userId","params","id","Number","userName","name","userIcon","icon","require","user","userPosts","filter","post","value","likedPosts","postId","appId","appName","appImage","avatar","tags","oneWard","comment","steamAppURL","favoriteCount","formattedDate","activeTab"],"sources":["/Users/namako-mac/mysite/game-introduction/src/views/AccountView.vue"],"sourcesContent":["<template>\n  <div class=\"account\">\n    <!-- ユーザー情報 -->\n    <div class=\"user-info\">\n      <img :src=\"user.icon\" alt=\"User Icon\" class=\"user-icon\" />\n      <div>\n        <h1 class=\"user-name\">{{ user.name }}</h1>\n        <p class=\"user-id\">@{{ user.id }}</p>\n      </div>\n    </div>\n\n    <!-- タブ切り替え -->\n    <div class=\"tab-toggle\">\n      <button :class=\"{ active: activeTab === 'accountPosts' }\" @click=\"activeTab = 'accountPosts'\">\n        投稿\n      </button>\n      <button :class=\"{ active: activeTab === 'likes' }\" @click=\"activeTab = 'likes'\">\n        いいね\n      </button>\n    </div>\n\n    <!-- 投稿表示 -->\n    <div v-if=\"activeTab === 'accountPosts'\" class=\"post-list\">\n      <PostItem :posts=\"userPosts\" />\n    </div>\n    <div v-else class=\"post-list\">\n      <PostItem :posts=\"likedPosts\" />\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, computed } from \"vue\";\nimport { useRoute }      from \"vue-router\"; // ルート情報を取得\nimport PostItem          from \"../components/PostItem.vue\";\nimport postData          from \"../postData\";\nimport defaultI       from \"../assets/userAvatar.png\";\n\n// ルートパラメータを取得\nconst route    = useRoute();\nconst userId   = route.params.id ? Number(route.params.id) : \"仮のID\";\nconst userName = route.params.name || \"デフォルトユーザー\";\nconst userIcon = route.params.icon || require();\n\n// ユーザー情報を指定\nconst user = ref({\n  id: userId,\n  name: userName,\n  icon: userIcon,\n});\n\n// ユーザーの投稿をフィルタリング\nconst userPosts = computed(() =>\n  typeof userId === \"number\"\n    ? postData.filter((post) => post.user.id === user.value.id)\n    : [] // 仮のIDの場合は空の投稿リスト\n);\n\n// ユーザーが「いいね」した投稿（仮のデータ）\nconst likedPosts = ref([\n  {\n    postId: 102,\n    appId: 1234567,\n    appName: \"Liked Game\",\n    appImage: [\"https://via.placeholder.com/150\"],\n    user: {\n      id: userId,\n      name: userName,\n      avatar: userIcon,\n    },\n    tags: [\"タグ1\", \"タグ2\"],\n    oneWard: \"お気に入りのゲーム\",\n    comment: \"このゲームは本当に素晴らしいです！\",\n    steamAppURL: \"https://store.steampowered.com/app/1234567/\",\n    favoriteCount: 100,\n    formattedDate: \"2025年5月15日\",\n  },\n]);\n\n// タブ切り替え状態\nconst activeTab = ref(\"accountPosts\");\n</script>\n\n<style scoped>\n.account {\n  display: flex;\n  flex-direction: column;\n  margin-right: 228px;\n  padding: 20px;\n}\n\n.user-info {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  gap: 15px;\n  margin-bottom: 15px;\n}\n\n.user-icon {\n  width: 70px;\n  height: 70px;\n  border-radius: 50%;\n}\n\n.user-name {\n  font-size: 2rem;\n  font-weight: bold;\n  margin: 0;\n}\n\n.user-id {\n  color: #657786;\n  font-size: 0.9rem;\n  margin: 2px 0 0;\n}\n\n.tab-toggle {\n  display: flex;\n  justify-content: center;\n  gap: 20px;\n  margin-bottom: 15px;\n  border-bottom: 1px solid #ccc;\n}\n\n.tab-toggle button {\n  background: none;\n  border: none;\n  padding: 10px 20px;\n  font-size: 1.5rem;\n  cursor: pointer;\n  color: #657786;\n  border-bottom: 2px solid transparent;\n  transition: all 0.3s ease;\n}\n\n.tab-toggle button.active {\n  color: #1da1f2;\n  border-color: #1da1f2;\n  font-weight: bold;\n}\n\n.post-list {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  margin-top: 30px;\n  gap: 15px;\n}\n</style>\n"],"mappings":";;AAgCA,SAASA,GAAG,EAAEC,QAAQ,QAAQ,KAAK;AA