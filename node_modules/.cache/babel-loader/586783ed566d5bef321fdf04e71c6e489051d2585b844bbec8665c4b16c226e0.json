{"ast":null,"code":"import { ref } from \"vue\";\nimport menuList from \"../menuList\";\nimport optionModalDialog from \"./optionModalDialog.vue\";\nexport default {\n  __name: 'HambergerMenu',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const active = ref(false); // ハンバーガーメニューのボタンのアニメーション\n    const show = ref(false); // ハンバーガーメニューの表示非表示\n    const isOptionModalOpen = ref(false); // モーダルの表示状態\n\n    const toggleMenu = () => {\n      active.value = !active.value; // ハンバーガーメニューの三本線とバツ印を切り替える\n      show.value = !show.value; // showの値を切り替える\n    };\n    const __returned__ = {\n      active,\n      show,\n      isOptionModalOpen,\n      toggleMenu,\n      ref,\n      get menuList() {\n        return menuList;\n      },\n      optionModalDialog\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","menuList","optionModalDialog","active","show","isOptionModalOpen","toggleMenu","value"],"sources":["/Users/namako-mac/mysite/game-introduction/src/components/HambergerMenu.vue"],"sourcesContent":["<template>\n  <div>\n    <!--ハンバーガーメニューのボタン-->\n    <div class=\"hamburger-btn\" @click=\"toggleMenu\">\n      <span class=\"line line-top\"    :class=\"{ 'rotate-top'   : active }\"></span>\n      <span class=\"line line-middle\" :class=\"{ 'rotate-middle': active }\"></span>\n      <span class=\"line line-bottom\" :class=\"{ 'rotate-bottom': active }\"></span>\n    </div>\n\n    <!--ハンバーガーメニューの中身-->\n    <transition name=\"hamburger-menu\">\n      <div class=\"hamburger-menu\" v-if=\"show\">\n        <ul>\n          <li v-for=\"menu in menuList\" :key=\"menu.id\">\n            <router-link :to=\"menu.path\">\n              <img :src=\"menu.src\" :alt=\"menu.alt\" />\n              {{ menu.name }}\n            </router-link>\n          </li>\n        </ul>\n      </div>\n    </transition>\n\n    <!--オプションモーダル-->\n    <OptionModalDialog/>\n  </div>\n</template>\n\n<script setup>\nimport { ref }  from \"vue\";\nimport menuList from \"../menuList\";\nimport optionModalDialog from \"./optionModalDialog.vue\";\n\nconst active = ref(false); // ハンバーガーメニューのボタンのアニメーション\nconst show   = ref(false); // ハンバーガーメニューの表示非表示\nconst isOptionModalOpen = ref(false); // モーダルの表示状態\n\nconst toggleMenu = () => {\n  active.value = !active.value; // ハンバーガーメニューの三本線とバツ印を切り替える\n  show.value   = !show.value;   // showの値を切り替える\n};\n</script>\n\n<style scoped>\n/* ハンバーガーメニューのボタンのcss */\n.hamburger-btn {\n  position: fixed;\n  top: 21px;\n  left: 11px;\n  width: 60px;\n  height: 60px;\n  cursor: pointer;\n  border: solid 2px #383e45;\n  border-radius: 10%;\n  background-color: rgb(240, 248, 255);\n  z-index: 50;\n}\n.line {\n  position: absolute;\n  top: 0;\n  left: 7.1px;\n  width: 45px;\n  height: 2px;\n  background: #333333;\n  text-align: center;\n}\n.line-top {\n  top: 20px;\n  transition: 0.4s ease;\n}\n.line-middle {\n  top: 30px;\n  transition: 0.4s ease;\n}\n.line-bottom {\n  top: 40px;\n  transition: 0.4s ease;\n}\n.rotate-top {\n  transform: translateY(10px) rotate(-45deg);\n  transition: 0.4s ease;\n}\n.rotate-middle {\n  transition: 0.4s ease;\n  opacity: 0;\n}\n.rotate-bottom {\n  transform: translateY(-10px) rotate(45deg);\n  transition: 0.4s ease;\n}\n\n/* ハンバーガーメニューの中身のcss */\n.hamburger-menu-enter-active,\n.hamburger-menu-leave-active {\n  transition: all 0.3s ease;\n}\n.hamburger-menu-enter-from,\n.hamburger-menu-leave-to {\n  transform: translateX(-100%);\n}\n.hamburger-menu-enter-to,\n.hamburger-menu-leave-from {\n  transform: translateX(0);\n}\n.hamburger-menu {\n  top: 0;\n  left: 0;\n  width: 160px;\n  height: 100vh;\n  padding: 2rem 1rem;\n  position: fixed;\n  background-color: rgb(8, 8, 30);\n  z-index: 30;\n}\nimg {\n  width: 32px;\n  height: 32px;\n  margin-right: 10px;\n  color: rgb(180, 180, 180);\n}\nimg:hover {\n  opacity: 0.7;\n}\nul {\n  padding: 0;\n  padding-top: 40px;\n}\nli {\n  padding: 10px 0;\n  list-style: none;\n  display: flex;\n  align-items: center;\n}\na {\n  font-size: 18px;\n  color: rgb(180, 180, 180);\n  text-decoration: none;\n  display: flex;\n  align-items: center;\n}\na:hover {\n  color: rgba(255, 255, 255, 0.8);\n}\n</style>\n"],"mappings":"AA6BA,SAASA,GAAG,QAAS,KAAK;AAC1B,OAAOC,QAAQ,MAAM,aAAa;AAClC,OAAOC,iBAAiB,MAAM,yBAAyB;;;;;;;IAEvD,MAAMC,MAAM,GAAGH,GAAG,CAAC,KAAK,CAAC,CAAC,CAAC;IAC3B,MAAMI,IAAI,GAAKJ,GAAG,CAAC,KAAK,CAAC,CAAC,CAAC;IAC3B,MAAMK,iBAAiB,GAAGL,GAAG,CAAC,KAAK,CAAC,CAAC,CAAC;;IAEtC,MAAMM,UAAU,GAAGA,CAAA,KAAM;MACvBH,MAAM,CAACI,KAAK,GAAG,CAACJ,MAAM,CAACI,KAAK,CAAC,CAAC;MAC9BH,IAAI,CAACG,KAAK,GAAK,CAACH,IAAI,CAACG,KAAK,CAAC,CAAG;IAChC,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}