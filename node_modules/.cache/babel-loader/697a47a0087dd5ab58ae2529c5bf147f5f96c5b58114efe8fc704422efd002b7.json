{"ast":null,"code":"import { reactive, computed } from \"vue\";\nimport PostItem from \"../components/PostItem.vue\";\nimport postData from \"../postData\";\n\n// 投稿データをリアクティブに管理\n\nexport default {\n  __name: 'RankingView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const posts = reactive(postData);\n\n    // ランキング上位10件を計算\n    const top10Posts = computed(() => {\n      return posts.slice() // 配列をコピーしてソートを安全に実行\n      .sort((a, b) => b.likeCount - a.likeCount) // likeCountで降順ソート\n      .slice(0, 10); // 上位10件を取得\n    });\n    const __returned__ = {\n      posts,\n      top10Posts,\n      reactive,\n      computed,\n      PostItem,\n      get postData() {\n        return postData;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["reactive","computed","PostItem","postData","posts","top10Posts","slice","sort","a","b","likeCount"],"sources":["/Users/namako-mac/mysite/game-introduction/src/views/RankingView.vue"],"sourcesContent":["<template>\n  <div class=\"ranking\">\n    <h1>RANKING</h1>\n    <ul class=\"ranking-list\">\n      <li v-for=\"(post, index) in top10Posts\" :key=\"post.postId\" class=\"ranking-item\">\n        <div class=\"ranking-frame\">\n          <!-- 順位を表示 -->\n          <div class=\"ranking-number\">\n            <p>{{ index + 1 }}</p>\n            </div>\n          <div class=\"post-item\">\n            <PostItem :posts=\"[post]\"/>\n          </div>\n        </div>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script setup>\nimport { reactive, computed } from \"vue\";\nimport PostItem               from \"../components/PostItem.vue\";\nimport postData               from \"../postData\";\n\n// 投稿データをリアクティブに管理\nconst posts = reactive(postData);\n\n// ランキング上位10件を計算\nconst top10Posts = computed(() => {\n  return posts\n    .slice() // 配列をコピーしてソートを安全に実行\n    .sort((a, b) => b.likeCount - a.likeCount) // likeCountで降順ソート\n    .slice(0, 10); // 上位10件を取得\n});\n</script>\n\n<style scoped>\n.ranking {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  padding: 20px;\n}\n\nh1 {\n  font-size: 3rem;\n  font-family: \"Arial, sans-serif\";\n  margin-bottom: 20px;\n}\n\n.ranking-list {\n  font-size: 3rem;\n  margin-bottom: 20px;\n  list-style: none;\n  padding: 0;\n}\n\n.ranking-frame {\n  display: flex;\n  align-items: center;\n  border: 1px solid #aaaaaa;\n  border-inline: 10px;\n  padding: 10000px;\n}\n\n.ranking-number {\n  font-size: 5rem;\n  font-weight: bold;\n  font-family: Arial, Helvetica, sans-serif;\n  margin-right: 20px;\n  width: 40px;\n  text-align: center;\n}\n\n/* PostItemのスタイルを統一 */\n.post-item {\n  flex: 1;\n  display: flex;\n  align-items: center;\n}\n</style>"],"mappings":"AAoBA,SAASA,QAAQ,EAAEC,QAAQ,QAAQ,KAAK;AACxC,OAAOC,QAAQ,MAAoB,4BAA4B;AAC/D,OAAOC,QAAQ,MAAoB,aAAa;;AAEhD;;;;;;;;IACA,MAAMC,KAAK,GAAGJ,QAAQ,CAACG,QAAQ,CAAC;;IAEhC;IACA,MAAME,UAAU,GAAGJ,QAAQ,CAAC,MAAM;MAChC,OAAOG,KAAI,CACRE,KAAK,CAAC,CAAC,CAAC;MAAA,CACRC,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKA,CAAC,CAACC,SAAS,GAAGF,CAAC,CAACE,SAAS,CAAC,CAAC;MAAA,CAC1CJ,KAAK,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC;IACnB,CAAC,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}