-weight: bold;\n}\n\n.post-list {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  margin-top: 30px;\n  gap: 15px;\n}\n</style>\n"],"mappings":";;AAsCA,SAASA,QAAQ,EAAEC,QAAQ,QAAQ,KAAK;AACxC,SAASC,QAAQ,QAAQ,YAAY;AACrC,OAAOC,QAAQ,MAAM,4BAA4B;AACjD,OAAOC,QAAQ,MAAM,aAAa;;AAElC;;;;;;;;IACA,MAAMC,KAAK,GAAGH,QAAQ,CAAC,CAAC;IACxB,MAAMI,MAAM,GAAGD,KAAK,CAACE,MAAM,CAACC,EAAE,GAAGC,MAAM,CAACJ,KAAK,CAACE,MAAM,CAACC,EAAE,CAAC,GAAG,IAAI;;IAE/D;IACA,MAAME,KAAK,GAAGV,QAAQ,CAACI,QAAQ,CAAC;;IAEhC;IACA,MAAMO,SAAS,GAAGV,QAAQ,CAAC,MACzBS,KAAK,CAACE,MAAM,CAAEC,IAAI,IAAKA,IAAI,CAACC,IAAI,CAACN,EAAE,KAAKF,MAAM,CAChD,CAAC;;IAED;IACA,MAAMS,UAAU,GAAGd,QAAQ,CAAC,MAC1BS,KAAK,CAACE,MAAM,CAAEC,IAAI,IAAKA,IAAI,CAACG,OAAO,CACrC,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}