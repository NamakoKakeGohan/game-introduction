{"ast":null,"code":"import { reactive, computed } from \"vue\";\nimport PostItem from \"../components/PostItem.vue\";\nimport postData from \"../postData\";\n\n// ÊäïÁ®ø„Éá„Éº„Çø„Çí„É™„Ç¢„ÇØ„ÉÜ„Ç£„Éñ„Å´ÁÆ°ÁêÜ\n\nexport default {\n  __name: 'RankingView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const posts = reactive(postData);\n\n    // „É©„É≥„Ç≠„É≥„Ç∞‰∏ä‰Ωç10‰ª∂„ÇíË®àÁÆó\n    const top10Posts = computed(() => {\n      return posts.slice() // ÈÖçÂàó„Çí„Ç≥„Éî„Éº„Åó„Å¶„ÇΩ„Éº„Éà„ÇíÂÆâÂÖ®„Å´ÂÆüË°å\n      .sort((a, b) => b.likeCount - a.likeCount) // likeCount„ÅßÈôçÈ†Ü„ÇΩ„Éº„Éà\n      .slice(0, 10); // ‰∏ä‰Ωç10‰ª∂„ÇíÂèñÂæó\n    });\n    const __returned__ = {\n      posts,\n      top10Posts,\n      reactive,\n      computed,\n      PostItem,\n      get postData() {\n        return postData;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["reactive","computed","PostItem","postData","posts","top10Posts","slice","sort","a","b","likeCount"],"sources":["/Users/namako-mac/mysite/game-introduction/src/views/RankingView.vue"],"sourcesContent":["<template>\n  <div class=\"ranking\">\n    <h1>üëëRANKING</h1>\n    <ul class=\"ranking-list\">\n      <li v-for=\"(post, index) in top10Posts\" :key=\"post.postId\" class=\"ranking-item\">\n        <div class=\"ranking-frame\">\n          <!-- È†Ü‰Ωç„ÇíË°®Á§∫ -->\n          <div class=\"ranking-number\">{{ index + 1 }}</div>\n          <!-- PostItem„Ç≥„É≥„Éù„Éº„Éç„É≥„Éà„Çí‰ΩøÁî® -->\n          <PostItem :posts=\"[post]\"/>\n        </div>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script setup>\nimport { reactive, computed } from \"vue\";\nimport PostItem               from \"../components/PostItem.vue\";\nimport postData               from \"../postData\";\n\n// ÊäïÁ®ø„Éá„Éº„Çø„Çí„É™„Ç¢„ÇØ„ÉÜ„Ç£„Éñ„Å´ÁÆ°ÁêÜ\nconst posts = reactive(postData);\n\n// „É©„É≥„Ç≠„É≥„Ç∞‰∏ä‰Ωç10‰ª∂„ÇíË®àÁÆó\nconst top10Posts = computed(() => {\n  return posts\n    .slice() // ÈÖçÂàó„Çí„Ç≥„Éî„Éº„Åó„Å¶„ÇΩ„Éº„Éà„ÇíÂÆâÂÖ®„Å´ÂÆüË°å\n    .sort((a, b) => b.likeCount - a.likeCount) // likeCount„ÅßÈôçÈ†Ü„ÇΩ„Éº„Éà\n    .slice(0, 10); // ‰∏ä‰Ωç10‰ª∂„ÇíÂèñÂæó\n});\n</script>\n\n<style scoped>\n.ranking {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  margin-right: 90px;\n  padding: 20px;\n}\nh1 {\n  font-size: 3rem;\n  margin-bottom: 20px;\n  color: #333;\n}\n\n.ranking-list {\n  font-size: 3rem;\n  margin-bottom: 20px;\n  list-style: none;\n  padding: 0;\n  color: #333;\n}\n\n.ranking-item {\n  \n}\n\n.ranking-frame {\n  display: flex;\n  align-items: center;\n  border: 1px solid #ddd;\n  border-inline: 10px;\n  padding: 10px;\n}\n\n.ranking-number {\n  font-size: 1.5rem;\n  font-weight: bold;\n  color: #333;\n  margin-right: 20px;\n  width: 40px;\n  text-align: center;\n}\n</style>"],"mappings":"AAiBA,SAASA,QAAQ,EAAEC,QAAQ,QAAQ,KAAK;AACxC,OAAOC,QAAQ,MAAoB,4BAA4B;AAC/D,OAAOC,QAAQ,MAAoB,aAAa;;AAEhD;;;;;;;;IACA,MAAMC,KAAK,GAAGJ,QAAQ,CAACG,QAAQ,CAAC;;IAEhC;IACA,MAAME,UAAU,GAAGJ,QAAQ,CAAC,MAAM;MAChC,OAAOG,KAAI,CACRE,KAAK,CAAC,CAAC,CAAC;MAAA,CACRC,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKA,CAAC,CAACC,SAAS,GAAGF,CAAC,CAACE,SAAS,CAAC,CAAC;MAAA,CAC1CJ,KAAK,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC;IACnB,CAAC,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}