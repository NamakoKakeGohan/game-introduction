{"ast":null,"code":"import { ref } from \"vue\";\n\n// 投稿カードコンポーネント\n\nexport default {\n  __name: 'AccountView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const PostCard = {\n      props: ['post'],\n      template: `\n    <div class=\"post-card\">\n      <p>{{ post.content }}</p>\n    </div>\n  `\n    };\n\n    // ユーザー情報\n    const user = ref({\n      name: \"John Doe\",\n      handle: \"johndoe\",\n      icon: \"https://via.placeholder.com/60\"\n    });\n\n    // 投稿といいねデータ\n    const userPosts = ref([{\n      id: 1,\n      content: \"今日はVueの勉強してる。\"\n    }, {\n      id: 2,\n      content: \"Composition APIは便利だけどちょっと慣れが必要。\"\n    }]);\n    const likedPosts = ref([{\n      id: 3,\n      content: \"明日は晴れるらしい ☀️\"\n    }, {\n      id: 4,\n      content: \"ChatGPTにコードレビュー頼んだら神だった。\"\n    }]);\n    const activeTab = ref(\"userPosts\");\n    const __returned__ = {\n      PostCard,\n      user,\n      userPosts,\n      likedPosts,\n      activeTab,\n      ref\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","PostCard","props","template","user","name","handle","icon","userPosts","id","content","likedPosts","activeTab"],"sources":["/Users/namako-mac/mysite/game-introduction/src/views/AccountView.vue"],"sourcesContent":["<template>\n  <div class=\"account\">\n    \n  </div>\n    <!-- ユーザー名とアイコン -->\n    <div class=\"user-info\">\n      <img :src=\"user.icon\" alt=\"User Icon\" class=\"user-icon\" />\n      <div>\n        <h1 class=\"user-name\">{{ user.name }}</h1>\n        <p class=\"user-handle\">@{{ user.handle }}</p>\n      </div>\n    </div>\n\n    <!-- 切り替えボタン -->\n    <div class=\"post-toggle\">\n      <button\n        :class=\"{ active: activeTab === 'userPosts' }\"\n        @click=\"activeTab = 'userPosts'\"\n      >\n        投稿\n      </button>\n      <button\n        :class=\"{ active: activeTab === 'likedPosts' }\"\n        @click=\"activeTab = 'likedPosts'\"\n      >\n        いいね\n      </button>\n    </div>\n\n    <!-- 投稿表示 -->\n    <div v-if=\"activeTab === 'userPosts'\" class=\"post-list\">\n            <PostItem :posts=\"filteredRecsList\" @post-click=\"openPostDetail\"/>\n\n    </div>\n\n    <div v-else class=\"post-list\">\n            <PostItem :posts=\"filteredRecsList\" @post-click=\"openPostDetail\"/>\n\n  </div>\n</template>\n\n<script setup>\nimport { ref } from \"vue\";\n\n// 投稿カードコンポーネント\nconst PostCard = {\n  props: ['post'],\n  template: `\n    <div class=\"post-card\">\n      <p>{{ post.content }}</p>\n    </div>\n  `\n};\n\n// ユーザー情報\nconst user = ref({\n  name: \"John Doe\",\n  handle: \"johndoe\",\n  icon: \"https://via.placeholder.com/60\",\n});\n\n// 投稿といいねデータ\nconst userPosts = ref([\n  { id: 1, content: \"今日はVueの勉強してる。\" },\n  { id: 2, content: \"Composition APIは便利だけどちょっと慣れが必要。\" },\n]);\n\nconst likedPosts = ref([\n  { id: 3, content: \"明日は晴れるらしい ☀️\" },\n  { id: 4, content: \"ChatGPTにコードレビュー頼んだら神だった。\" },\n]);\n\nconst activeTab = ref(\"userPosts\");\n</script>\n\n<style scoped>\n.account {\n  padding: 20px;\n  max-width: 600px;\n  margin: 0 auto;\n  font-family: 'Helvetica Neue', Arial, sans-serif;\n  background-color: #fff;\n}\n\n.user-info {\n  display: flex;\n  align-items: center;\n  gap: 15px;\n  margin-bottom: 15px;\n}\n\n.user-icon {\n  width: 60px;\n  height: 60px;\n  border-radius: 50%;\n}\n\n.user-name {\n  font-size: 1.2rem;\n  font-weight: bold;\n  margin: 0;\n}\n\n.user-handle {\n  color: #657786;\n  font-size: 0.9rem;\n  margin: 2px 0 0;\n}\n\n.post-toggle {\n  display: flex;\n  gap: 20px;\n  margin-bottom: 15px;\n  border-bottom: 1px solid #ccc;\n}\n\n.post-toggle button {\n  background: none;\n  border: none;\n  padding: 10px 0;\n  font-size: 1rem;\n  cursor: pointer;\n  color: #657786;\n  border-bottom: 2px solid transparent;\n  transition: all 0.3s ease;\n}\n\n.post-toggle button.active {\n  color: #1da1f2;\n  border-color: #1da1f2;\n  font-weight: bold;\n}\n\n.post-list {\n  display: flex;\n  flex-direction: column;\n  gap: 15px;\n}\n\n.post-card {\n  padding: 15px;\n  background: #fff;\n  border: 1px solid #eee;\n  border-radius: 10px;\n  box-shadow: 0 2px 5px rgba(0,0,0,0.05);\n  transition: background-color 0.2s;\n}\n\n.post-card:hover {\n  background-color: #f5f8fa;\n}\n\n.post-card p {\n  margin: 0;\n  color: #14171a;\n  font-size: 1rem;\n  line-height: 1.4;\n}\n</style>\n"],"mappings":"AA0CA,SAASA,GAAG,QAAQ,KAAK;;AAEzB;;;;;;;;IACA,MAAMC,QAAQ,GAAG;MACfC,KAAK,EAAE,CAAC,MAAM,CAAC;MACfC,QAAQ,EAAE;AACZ;AACA;AACA;AACA;IACA,CAAC;;IAED;IACA,MAAMC,IAAI,GAAGJ,GAAG,CAAC;MACfK,IAAI,EAAE,UAAU;MAChBC,MAAM,EAAE,SAAS;MACjBC,IAAI,EAAE;IACR,CAAC,CAAC;;IAEF;IACA,MAAMC,SAAS,GAAGR,GAAG,CAAC,CACpB;MAAES,EAAE,EAAE,CAAC;MAAEC,OAAO,EAAE;IAAgB,CAAC,EACnC;MAAED,EAAE,EAAE,CAAC;MAAEC,OAAO,EAAE;IAAkC,CAAC,CACtD,CAAC;IAEF,MAAMC,UAAU,GAAGX,GAAG,CAAC,CACrB;MAAES,EAAE,EAAE,CAAC;MAAEC,OAAO,EAAE;IAAe,CAAC,EAClC;MAAED,EAAE,EAAE,CAAC;MAAEC,OAAO,EAAE;IAA2B,CAAC,CAC/C,CAAC;IAEF,MAAME,SAAS,GAAGZ,GAAG,CAAC,WAAW,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}